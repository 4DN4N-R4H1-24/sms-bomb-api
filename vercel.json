{{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/bomber",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ]
}
  "version": 2,
  "builds": [
    {
      "src": "api/bomber.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bomber",
      "dest": "/api/bomber.js"
    }
  ]
}
